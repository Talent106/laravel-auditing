{
    "name": "wilianx7/laravel-auditing",
    "description": "Audit changes of your Eloquent models in Laravel/Lumen. Forked from owen-t/laravel-auditing.",
    "keywords": [
        "accountability",
        "audit",
        "auditing",
        "changes",
        "eloquent",
        "history",
        "log",
        "logging",
        "observer",
        "laravel",
        "lumen",
        "record",
        "revision",
        "tracking"
    ],
    "homepage": "https://github.com/wilianx7/laravel-auditing",
    "type": "package",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/wilianx7/laravel-auditing/issues",
        "source": "https://github.com/wilianx7/laravel-auditing"
    },
    "authors": [
        {
            "name": "Wilian Hendler",
            "email": "wilianx7@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/console": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/filesystem": "^7.0|^8.0|^9.0|^10.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0",
        "laravel/legacy-factories": "*"
    },
    "autoload": {
        "psr-4": {
            "wilianx7\\Auditing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "wilianx7\\Auditing\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v13-dev"
        },
        "laravel": {
            "providers": [
                "wilianx7\\Auditing\\AuditingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
